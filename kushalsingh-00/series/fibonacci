#include<stdio.h>
int main()
{
    //0 1 1 2 3 5 8
    int a=0;
    int b=1;
    int c,d;

    printf("write the up to which you want fibonacci");
    scanf("%d",&d);
    
    printf("%d %d ",a,b);
    for(int i=1;i<=d-2;i++)
    {
        c=a+b;
        a=b;
        b=c;
        printf("%d ",c);
    
    }
}
